<h1>Portfolio Items</h1>

<h2><%= link_to 'Create new Item', new_portfolio_page_path %></h2>

<h2><% @portfolio_item.each do |p| %>
      <p><%= p.title %></p>
      <p><%= p.subtitle %></p>
      <p><%= p.body %></p>
      <%= image_tag p.thumb_image if !p.thumb_image.nil? %>
      <%= link_to 'edit', edit_portfolio_page_path(p.id) %>
    <% end %>
</h2>